                        "name": "gRPCInvoke",
                        "description": "gRPC client activity",
                        "activity": {
                            "ref": "#grpc",
                            "settings": {
                                "operatingMode": "grpc-to-grpc",
                                "hosturl": "localhost:8087",
                                "enableTLS": false,
                                "clientCert": "",
